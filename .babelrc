{
  "comments": false,
  "env": {
    "test": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    },
    "main": {
      "presets": [
        [
          "@babel/env",
          {
            "targets": {
              "node": 7
            }
          }
        ],
        "minify"
      ]
    },
    "renderer": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false
          }
        ],
        "minify"
      ]
    },
    "web": {
      "presets": [
        [
          "@babel/env",
          {
            "modules": false
          }
        ],
        "minify"
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
